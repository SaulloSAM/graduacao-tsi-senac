<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - GANDALF</title>
    <script src="http://code.jquery.com/jquery-1.12.4.min.js"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>

<body>
    @Html.Partial("Header")
    @RenderBody()
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/app")
    @RenderSection("scripts", required: false)
    @Html.Partial("Footer")

    <script>
        var categoria = ["Caneca", "Games", "Jogos", "Quadrinhos", "Colecionaveis", "Máscara de Dormir", "Decoração"];
        $("#principalSearch").autocomplete({ source: categoria });
        // $("#imgDialog").dialog({ modal: true, hide: { effect: "explode", duration: 1000 } });

        $(".cancel-cart").empty();
        $(".cancel-carte").html("<i class='icon-remove-sign'></i>");
    </script>

    <style>
        p.btn-add { cursor: pointer; }
    </style>
</body>
</html>